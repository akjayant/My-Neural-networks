   },
   "outputs": [],
   "source": [
    "weights = []\n",
    "graph_w = []\n",
    "graph_b = []\n",
    "for j in range(4):  \n",
    "    weights.append(find_weight(j,w1,b1,alpha)[0])\n",
    "    graph_w.append(find_weight(j,w1,b1,alpha)[1])\n",
    "    graph_b.append(find_weight(j,w1,b1,alpha)[2])"
   ]
